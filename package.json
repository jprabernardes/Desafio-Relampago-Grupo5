{
  "name": "sistema-academia",
  "version": "1.0.0",
  "description": "Sistema de gerenciamento de academia com 4 roles",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/server.ts",
    "test": "mocha -r ts-node/register tests/**/*.test.ts --reporter json --reporter-options output=reports/test-report.json",
    "test:watch": "mocha --require ts-node/register 'tests/**/*.test.ts' --watch",
    "test:unit": "mocha --require ts-node/register 'tests/unit/**/*.test.ts'",
    "test:integration": "mocha --require ts-node/register 'tests/integration/**/*.test.ts'",
    "test:coverage": "nyc npm test",
    "test:report": "nyc --reporter=html --reporter=text npm test",
    "start": "node dist/server.js",
    "seed": "ts-node src/database/seed.ts"
  },
  "keywords": [
    "academia",
    "gym",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.4",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.3.1",
    "express": "^4.22.1",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^4.3.20",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "chai": "^4.5.0",
    "mocha": "^10.8.2",
    "nyc": "^17.1.0",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  },
  "directories": {
    "test": "tests"
  }
}
